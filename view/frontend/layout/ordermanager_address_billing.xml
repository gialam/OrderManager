<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Magenest\OrderManager\Block\Address\Billing\Edit" name="order.billing.edit"
                   template="Magenest_OrderManager::order/billing/info.phtml">
            </block>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back"
                   template="account/link/back.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
